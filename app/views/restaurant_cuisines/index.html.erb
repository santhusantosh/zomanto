<p id="notice"><%= notice %></p>

<h1>Listing Restaurant Cuisines</h1>

<table>
  <thead>
    <tr>
      <th>Restaurant</th>
      <th>Cuisine</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @restaurant_cuisines.each do |restaurant_cuisine| %>
      <tr>
        <td><%= restaurant_cuisine.restaurant_id %></td>
        <td><%= restaurant_cuisine.cuisine_id %></td>
        <td><%= link_to 'Show', restaurant_cuisine %></td>
        <td><%= link_to 'Edit', edit_restaurant_cuisine_path(restaurant_cuisine) %></td>
        <td><%= link_to 'Destroy', restaurant_cuisine, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Restaurant cuisine', new_restaurant_cuisine_path %>
